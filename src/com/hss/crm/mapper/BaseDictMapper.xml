<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hss.crm.mapper.BaseDictMapper">
    <select id="queryBaseDictByDictTypeCode" parameterType="String" resultType="com.hss.crm.pojo.BaseDict">
      select * from base_dict where dict_type_code = #{dict_type_code}
    </select>
</mapper>